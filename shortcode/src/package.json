{
  "name": "gulpfile_generator",
  "version": "1.1.0",
  "description": "This is a genarator for gulpfile and package.json",
  "author": "",
  "url": "",
  "license": "MIT License",
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e js-vue/main.js -o assets/js/build.js",
    "serve": "http-server -o -s -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify js-vue/main.js | uglifyjs -c warnings=false -m > ../assets/js/build.js",
    "compilation": "browserify js-vue/main.js -o ../assets/js/build.js"
  },
  "dependencies": {
    "vue": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "group-css-media-queries": "*",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "*",
    "gulp-babel": "^7.0.1",
    "gulp-cache": "*",
    "gulp-clean-css": "*",
    "gulp-concat": "*",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-imagemin": "*",
    "gulp-plumber": "*",
    "gulp-print": "^2.0.1",
    "gulp-rename": "*",
    "gulp-sass": "^4.0.1",
    "gulp-sass-glob": "^1.0.9",
    "gulp-shell": "^0.6.5",
    "gulp-uglify": "*",
    "gulp-util": "^3.0.8",
    "http-server": "^0.9.0",
    "imagemin-pngquant": "^5.0.1",
    "npm-run-all": "^2.1.2",
    "smart-grid": "^2.0.1",
    "uglify-js": "^2.5.0",
    "vinyl-ftp": "^0.6.1",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  }
}
